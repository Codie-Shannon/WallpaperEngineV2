        <!-- =============================================================== -->
        <Style x:Key="Installed.Selection.Block.Grid" TargetType="Grid">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="Visibility" Value="Collapsed"/>
            <Setter Property="IsHitTestVisible" Value="True"/>
